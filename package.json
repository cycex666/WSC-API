  "scripts": {
    "postinstall": "npx tsc",
    "clean": "del-cli .nyc_output coverage dist build.zip",
    "build.assets": "npm run \"clean\" && cpy \"**/*.json\" \"**/*.xml\" \"**/.env\" \"../dist\" --cwd=src --parents",
