    "test": "npm run lint && ava test --concurrency 1",
    "dev": "npm run \"build.assets\" &&  tsc-watch --onSuccess \"node ./dist/server.js\"",
    "start": "npx pm2 start pm2.yaml",
    "build.docs": "apidoc -i src -o docs"
  },
